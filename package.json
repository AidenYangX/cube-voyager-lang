{
  "name": "cube-voyager-lang",
  "displayName": "cube-voyager-lang",
  "description": "A Language Support for Cube Voyager",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cvoyager",
      "aliases": ["Cube Voyager", "cvoyager"],
      "extensions": [".s"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cvoyager",
      "scopeName": "source.s",
      "path": "./syntaxes/cvoyager.tmLanguage.json"
    }]
  }
}
